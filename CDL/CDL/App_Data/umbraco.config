      <script type="text/javascript">
          $(document).ready(function () {
              var regexExpression = /^[a-zA-Z0-9!#$%&()*+,-.\/\s:;<=>?@\^_`|~]+$/;
              var RegExEmail = /^([0-9a-zA-Z]([-\.\w]*[0-9a-zA-Z])*@([0-9a-zA-Z][-\w]*[0-9a-zA-Z]\.)+[a-zA-Z]{2,9})$/;
              var RegExPhone = /^([0-9\(\)\/\+ \-]*)$/;  //only numeric characters and + - sign

              clearObject();
              hideObject();
              $("#Name").keypress(function (e) {
                  var keycode = (e.keyCode ? e.keyCode : e.which);
                  if (keycode == '13') {
                      e.preventDefault();
                      $('#e-name').hide();
                  }
              });
              $("#Email").keypress(function (e) {
                  var keycode = (e.keyCode ? e.keyCode : e.which);
                  if (keycode == '13') {
                      e.preventDefault();
                      $("#e-email").hide();
                  }
              });
              $("#PhoneNumber").keypress(function (e) {
                  var keycode = (e.keyCode ? e.keyCode : e.which);
                  if (keycode == '13') {
                      e.preventDefault();
                      $("#e-phone").hide();
                  }
              });

              $("#Name").blur(function (e) {
                  var $this = $(this);
                  e.preventDefault();
                  if ($this.val() != '') {
                      $('#e-name').hide();
                  }
              });

              $("#Email").blur(function (e) {
                  var $this = $(this);
                  e.preventDefault();
                  if ($this.val() != '' && RegExEmail.test($this.val())) {
                      $("#e-email").hide();
                  }
              });

              $("#PhoneNumber").blur(function (e) {
                  var $this = $(this);
                  e.preventDefault();
                  if ($this.val() != '') {
                      $("#e-phone").hide();
                  }
              });

              $("#btnSend").click(function (e) {
                  e.preventDefault();
                  var rawUrl = window.location.pathname;
                  var siteName = "";
                  var n = rawUrl.indexOf("/");
                  var n1 = rawUrl.indexOf("/", parseInt(n + 1));
                  siteName = rawUrl.substring(1, n1);
                  siteName = toTitleCase(siteName.replace("-", " "));

                  if (validate()) {
                      $('#imgDiv').show();//show your loading image
                      var name = $("#Name").val();
                      var email = $("#Email").val();
                      var phone = $("#PhoneNumber").val();
                      var formdata = { Name: name, Email: email, PhoneNumber: phone, SiteName: siteName };

                      //console.log(formdata);
                      $.ajax({
                          method: "GET",
                          url: "/handler/floorPlansHandler.ashx",
                          data: formdata
                      })
                      .done(function (msg) {
                          //console.log(msg);
                          location.href = "/nouvel18/thanks.html"
                      })
                      .fail(function () {
                          //alert( "error" );
                          location.reload();
                      });

                  } else {
                      //not to submit form
                      e.preventDefault();
                  }
              });

              function toTitleCase(str) {
                  return str.replace(/\w\S*/g, function (txt) { return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase(); });
              }

              //----- validation

              function validate() {

                  var txtName = $("#Name").val();
                  var txtEmail = $("#Email").val();
                  var txtPhone = $("#PhoneNumber").val();

                  var eName = false;
                  var eEmail = false;
                  var ePhone = false;

                  document.getElementById("e-name").innerHTML = "";
                  document.getElementById("e-email").innerHTML = "";
                  document.getElementById("e-phone").innerHTML = "";


                  var validate = "";
                  //var msg = "Please fill in all fields.";

                  if (txtName == null || txtName == "") {
                      document.getElementById("e-name").innerHTML = "Please provide your name.";
                      $('#e-name').show();
                      eName = true;
                      validate = "invalid";
                  }
                  else { $('#e-name').hide(); }

                  if (txtEmail == null || txtEmail == "") {
                      document.getElementById("e-email").innerHTML = "Please provide a valid email address.";
                      $('#e-email').show();
                      eEmail = true;
                      validate = "invalid";
                  } else {
                      if (!RegExEmail.test(txtEmail)) {
                          document.getElementById("e-email").innerHTML = "Please enter valid email eg. yourname@yahoo.com";
                          $('#e-email').show();
                          eEmail = true;
                          validate = "invalid";
                      }
                      else {
                          $('#e-email').hide();
                      }
                  }

                  if (txtPhone == null || txtPhone == "") {
                      document.getElementById("e-phone").innerHTML = "Please provide a valid contact number.";
                      $('#e-phone').show();
                      ePhone = true;
                      validate = "invalid";
                  } else {
                      if (!RegExPhone.test(txtPhone)) {
                          document.getElementById("e-phone").innerHTML = "Please provide a valid contact number.";
                          $('#e-phone').show();
                          ePhone = true;
                          validate = "invalid";
                      }
                      else {
                          $('#e-phone').hide();
                      }

                  }

                  if (validate == 'invalid') {

                      if (eName) {
                          $("#Name").focus();
                      }
                      else if (eEmail) {
                          $("#Email").focus();
                      } else if (ePhone) {
                          $("#PhoneNumber").focus();
                      }

                      return false;
                  }


                  return true;
              }

              //----- validation

              function hideObject() {
                  $('#e-name').hide();
                  $("#e-email").hide();
                  $("#e-phone").hide();

              }
              function clearObject() {
                  $("#Name").val("");
                  $("#Email").val("");
                  $("#PhoneNumber").val("");
              }
          });
</script>
